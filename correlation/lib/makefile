libcoral.a : coral.o
	ar -ru libcoral.a coral.o
coral.o : ${CORALINC}/coral.cc ${HCFILES}
	${CPP} -c -FPIC -I${CORALINC} ${OPT} -o coral.o ${CORALINC}/coral.cc
clean :
	rm -f *.o *~
